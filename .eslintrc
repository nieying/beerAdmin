{
	"env": {
		"browser": true,
		"jest": true,
		"es6": true,
		"node": true
	},
	"parser": "babel-eslint",
	"plugins": [
		"react",
		"import"
	],
	"extends": "airbnb",
	"rules": {
		"jsx-a11y/anchor-is-valid": ["error", {
			"components": [],
			"specialLink": [],
			"aspects": ["noHref", "invalidHref", "preferButton"]
		}],
		"quotes": [1, "double"],
		"import/no-unresolved": "off",
		"import/extensions": "off",
		"import/no-extraneous-dependencies": "off"
	}
}
